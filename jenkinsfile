pipeline
{
	agent any 
	stages
		{
			stage('git clone')
			{
				steps
				{
					git 'https://github.com/DhanshreeBorase/maven-project.git'
				}
			}
			stage ('compile my project')
			{
				steps 
				{
					withMaven(jdk: 'localJDK' , maven: 'localMaven')
					{
						sh 'mvn compile'
					}
				}
			}
			stage ('test my project')
			{
				steps 
				{
					withMaven(jdk: 'localJDK' , maven: 'localMaven')
					{
						sh 'mvn test'
					}
				}
			}
		}
}
